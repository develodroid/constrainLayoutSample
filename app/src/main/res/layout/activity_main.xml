<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.develodroid.constraintslayoutsample.MainActivity">

    <ImageView
        android:layout_width="0dp"
        android:layout_height="257dp"
        app:srcCompat="@mipmap/main_header"
        android:id="@+id/imageView8"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/activity_main"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        tools:layout_constraintRight_creator="1" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@mipmap/ic_edit"
        android:id="@+id/floatingActionButton2"
        app:elevation="7dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        tools:layout_constraintBottom_creator="1" />

    <ImageView
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@mipmap/ic_phone"
        android:id="@+id/imageView9"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        android:layout_marginTop="48dp" />

    <TextView
        android:text="@string/at_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        tools:text="Finn The Human"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        android:textStyle="normal|bold"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        android:layout_marginStart="96dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        android:layout_marginEnd="96dp"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:text="@string/at_phone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        tools:text="555 - 000 - 000"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintLeft_toLeftOf="@+id/textView6"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9" />

    <ImageView
        android:layout_width="48dp"
        android:layout_height="43dp"
        app:srcCompat="@mipmap/ic_marker"
        android:id="@+id/imageView10"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        android:layout_marginTop="84dp"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        android:layout_marginBottom="86dp"
        tools:layout_constraintBottom_creator="1" />

    <TextView
        android:text="@string/at_loc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        tools:text="CANDYLAND"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        android:textStyle="normal|bold"
        app:layout_constraintLeft_toRightOf="@+id/imageView10"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toTopOf="@+id/imageView10"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView10"
        app:layout_constraintHorizontal_bias="0.13" />

    <ImageView
        android:layout_width="0dp"
        app:srcCompat="@mipmap/ic_web"
        android:id="@+id/imageView11"
        android:layout_height="48dp"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageView10"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/activity_main"
        android:layout_marginBottom="16dp" />

    <TextView
        android:text="@string/at_web"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        tools:text="@string/at_web"
        app:layout_constraintLeft_toLeftOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.52" />

    <ImageView
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@mipmap/ic_mail"
        android:id="@+id/imageView12"
        app:layout_constraintLeft_toLeftOf="@+id/activity_main"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_main"
        android:layout_marginBottom="35dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:text="@string/at_mail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        tools:text="finnthehuman@gmail.com"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintLeft_toLeftOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/imageView12"
        app:layout_constraintRight_toRightOf="@+id/activity_main"
        app:layout_constraintBottom_toBottomOf="@+id/imageView12"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.58000004" />

</android.support.constraint.ConstraintLayout>
